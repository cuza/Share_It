<h1>Folder</h1>

<table class="table table-condensed table-striped table-hover table-bordered">
    <tbody>
    <tr>
        <th>Id</th>
        <td>{{ entity.id }}</td>
    </tr>
    <tr>
        <th>Name</th>
        <td>{{ entity.name }}</td>
    </tr>
    <tr>
        <th>Creation Date</th>
        <td>{{ entity.c_date|date('Y-m-d H:i:s') }}</td>
    </tr>
    <tr>
        <th>Files</th>
        <td>{{ entity.files }}</td>
    </tr>
    <tr>
        <th>Folders</th>
        <td>{{ entity.folders }}</td>
    </tr>
    </tbody>
</table>